{
    "name": "lukaswhite/temporary-files",
    "description": "Self-destructing temporary files for Laravel",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "lukaswhite",
            "email": "hello@lukaswhite.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.5|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
